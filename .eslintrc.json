{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "import/prefer-default-export": "off",
        "no-multi-assign": "off",
        "arrow-parens": "off"
    },
    "ignorePatterns": [
        "dist/",
        "build/"
    ]
}